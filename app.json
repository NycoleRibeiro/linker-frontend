{
  "expo": {
    "name": "Linker",
    "slug": "linker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/iconColor.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/iconColor.png",
        "backgroundColor": "#18181b"
      }
    },
    "web": {
      "favicon": "./assets/img/iconWhite.png"
    },
    "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "The app accesses your photos to let you share them with your friends."
          }
        ]
      ]
  }
}
